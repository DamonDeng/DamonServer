<%= form_for :dfile ,url: dfiles_path,multipart:true do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>

  <input type="file" name="file[file]">
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
